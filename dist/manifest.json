{
  "manifest_version": 3,
  "name": "Language Learning Extension",
  "version": "1.0.0",
  "description": "YouTube Language Learning Overlay",
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-l0sNRNKZ.js"
      ],
      "matches": [
        "https://www.youtube.com/watch*"
      ]
    }
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "resources": [
        "assets/index.ts-l0sNRNKZ.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
